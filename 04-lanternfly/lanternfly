import java.util.Scanner;
public class lanternfly
{
    public static void main(String[]args){
     Scanner input=new Scanner(System.in);
     System.out.println("The lanternfly is an invasive species and");
     System.out.println("is a threat to our trees and plant.");
     System.out.println("How many did you squash this summer?");
     int num=input.nextInt();
     if (num<0){
        System.out.println("Why did you save them");
    }//end of if statement
     else if (num<10){
        System.out.println("Squash more");
    }//end of if statement
     else if (100<num){
        System.out.println("Good job");
     }
    else{
      System.out.println("You can do better");      
    }//end else statment    
  }
 }         
